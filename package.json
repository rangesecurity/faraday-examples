{
  "name": "faraday-examples",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Examples for interacting with the Faraday API via faraday-sdk",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "dev:health": "tsx src/health.ts",
    "dev:chains": "tsx src/chains.ts",
    "dev:quote": "tsx src/quote.ts",
    "dev:submit": "tsx src/submit.ts",
    "dev:tokens": "tsx src/tokens.ts",
    "build": "tsc -p tsconfig.json",
    "start:health": "node dist/health.js"
  },
  "dependencies": {
    "@ethersproject/rlp": "^5.8.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "faraday-sdk": "file:../faraday-sdk"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "engines": {
    "node": ">=18"
  }
}
